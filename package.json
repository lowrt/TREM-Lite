{
  "name": "trem_lite",
  "version": "1.4.0-rc.1",
  "description": "Taiwan Real-time Earthquake Monitoring ( 臺灣即時地震監測 )",
  "main": "main.js",
  "homepage": "https://exptech.com.tw/",
  "author": "ExpTechTW <ExpTech.tw@gmail.com>",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dist": "electron-builder",
    "lint": "eslint --ext .js js/"
  },
  "build": {
    "productName": "TREM-Lite",
    "appId": "tw.exptech.trem_lite",
    "asar": false,
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": null,
      "target": [],
      "icon": "TREM.ico"
    },
    "linux": {
      "publish": null,
      "target": "deb",
      "category": "Development",
      "maintainer": "ExpTechTW"
    },
    "directories": {
      "buildResources": "resources",
      "output": "dev"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExpTechTW/TREM-Plus.git"
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "devDependencies": {
    "electron": "23.1.3",
    "electron-builder": "^24.0.0",
    "eslint": "^8.35.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "electron-common-ipc": "^16.0.1-dev.7",
    "electron-fcm-push-receiver": "^2.1.7",
    "eslint-plugin-require-sort": "^1.3.0",
    "expose-gc": "^1.0.0",
    "leaflet": "^1.9.3",
    "leaflet-edgebuffer": "^1.0.6",
    "leaflet-geojson-vt": "1.1.0",
    "node-datachannel": "^0.4.1",
    "node-fetch": "^2.6.7",
    "ws": "^8.12.1"
  }
}
